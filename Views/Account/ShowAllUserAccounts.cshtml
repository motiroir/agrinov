@model List<AgriNov.ViewModels.UserAccountViewModel>
@using AgriNov.Models

@{
    Layout = "_BaseLayout";
    ViewBag.Title = "Tous les utilisateurs";
}
@section ExtraStyle {
    <link rel="stylesheet" href="~/CSS/tablestyle.css">
}


<div class="table-container">

    <table style="--colNbr:5">
        <thead>
            <tr>
                <th>Nom</th>
                <th>Mail</th>
                <th>Status</th>
                <th>Date d'inscription</th>
                <th>Voir plus</th>
            </tr>
        </thead>
        <tbody>
            @foreach (AgriNov.ViewModels.UserAccountViewModel userAccountViewModel in Model)
            {
               <tr>
                    <td>ContactName</td>
                    <td>@userAccountViewModel.UserAccount.Mail</td>
                    <td>@userAccountViewModel.UserAccount.UserAccountLevel.GetDisplayName()</td>
                    <td>@userAccountViewModel.UserAccount.DateCreated.ToString("dd/MM/yyyy")</td>
                    <td>SVG oeil</td>
                </tr>
            }
        </tbody>
    </table>



</div>

